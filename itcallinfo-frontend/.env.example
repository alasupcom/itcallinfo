# ITCallInfo Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
VITE_API_BASE_URL=http://your-api-host:5000

# Deployment Configuration
DEPLOYMENT_HOST=your-deployment-server-host
DEPLOYMENT_USER=your-deployment-user
DEPLOYMENT_PATH=/var/www/html/itcallinfo

# Application Configuration
VITE_APP_NAME=ITCallInfo
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false 